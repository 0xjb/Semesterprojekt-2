<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="presentation.ControllerFXMLSocialWorker">
   <children>
      <TabPane prefHeight="300.0" prefWidth="720.0" tabClosingPolicy="UNAVAILABLE">
         <tabs>
            <Tab fx:id="casesTab" text="Cases">
               <content>
                  <HBox prefHeight="100.0" prefWidth="200.0">
                     <children>
                        <VBox alignment="CENTER" prefHeight="271.0" prefWidth="517.0" spacing="10.0">
                           <children>
                              <Button fx:id="swCasesCreateCaseButton" mnemonicParsing="false" onAction="#createCaseAction" text="Create case" />
                              <Button mnemonicParsing="false" text="Edit case" />
                           </children>
                        </VBox>
                        <VBox alignment="CENTER" prefHeight="200.0" prefWidth="135.0" spacing="10.0">
                           <children>
                              <ListView fx:id="caseListView" maxWidth="-Infinity" prefHeight="200.0" prefWidth="125.0" />
                              <Button fx:id="swDeleteCaseListButton" mnemonicParsing="false" onAction="#DeleteCaseAction" text="Delete case" />
                              <Button fx:id="swUpdateCaseListButton" mnemonicParsing="false" onAction="#UpdateCaseListAction" text="Update List" />
                           </children>
                           <padding>
                              <Insets right="10.0" />
                           </padding>
                        </VBox>
                     </children>
                  </HBox>
               </content>
            </Tab>
            <Tab fx:id="inquiriesTab" text="Inquiries">
              <content>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
              </content>
            </Tab>
            <Tab fx:id="referenceTab" text="References">
              <content>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
              </content>
            </Tab>
         </tabs>
      </TabPane>
   </children>
</AnchorPane>
